{
	"manifest_version": 2,
	"name": "search_urban_dictionary",
	"version": "0.1",
	"description": "Search Urban Dictionary",

	"icons": {
		"48": "icons/icon.png",
		"96": "icons/icon@2x.png"
	},

	"chrome_settings_overrides": {
	  "search_provider": {
		"name": "Urban Dictionary",
		"search_url": "https://www.urbandictionary.com/define.php?term={searchTerms}",
		"keyword": "u",
		"favicon_url": "/icons/favicon.ico"
	  }
	},

	"browser_specific_settings": {
		"gecko": {
			"id": "firefox_search_urban_dictionary@actionless.github.com",
			"strict_min_version": "106.0"
		}
	}
}
